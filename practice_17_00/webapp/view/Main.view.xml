<mvc:View controllerName="practice1700.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:l="sap.ui.layout"
    xmlns:c="sap.ui.core"
    xmlns:t="sap.ui.table"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">

        <Panel headerText="프로젝트 2 - 계산기 테이블" expandable="true" expanded="false">                
            <mvc:XMLView viewName="practice1700.view.Cal" />
        </Panel>

        <Panel headerText="프로젝트 6 - Fragment 📢 Dialog 호출" expandable="true" expanded="false">            
            <Button text="Open Dialog" press="onOpenNameDialog"/>
        </Panel>
        <c:Fragment fragmentName="practice1700.view.fragment.NameDialog"/>

        <Panel headerText="프로젝트 8 - 선택 셀 삭제" expandable="true" expanded="false">            
            <mvc:XMLView viewName="practice1700.view.SelectCell" />
        </Panel>


        <Panel headerText="프로젝트 9 - 필터링 검색 🔍 Routing" expandable="true" expanded="false">            
            <mvc:XMLView viewName="practice1700.view.Filtering" />
        </Panel>

        
        <Panel headerText="프로젝트 14 - Member에 CRUD" expandable="true" expanded="true">            
            <mvc:XMLView viewName="practice1700.view.ODataMember" />
        </Panel>

        

    </Page>
</mvc:View>
